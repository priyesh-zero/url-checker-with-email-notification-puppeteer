{
  "name": "url-checker",
  "version": "1.0.0",
  "description": "It looks for a list of urls and then check through puppeteer weather they are working or not, also send an email to a user as a notificaiton.",
  "main": "index.js",
  "scripts": {
    "securitiyOff": "set NODE_TLS_REJECT_UNAUTHORIZED=0",
    "securitiyOn": "set NODE_TLS_REJECT_UNAUTHORIZED=1",
    "start": "node index.js >> logs\\latest.log",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Puppeteer",
    "Node",
    "NodeMailer",
    "Url"
  ],
  "author": "Priyesh Shrivastava",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "nodemailer": "^6.4.6",
    "puppeteer": "^3.0.4"
  }
}
